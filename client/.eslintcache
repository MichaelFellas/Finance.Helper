 <span>{skill}</span>\n                  {isLoggedInUser && (\n                    <button\n                      className=\"btn btn-sm btn-danger ml-auto\"\n                      onClick={() => handleRemoveSkill(skill)}\n                    >\n                      X\n                    </button>\n                  )}\n                </h4>\n              </div>\n            </div>\n          ))}\n      </div>\n      {error && (\n        <div className=\"my-3 p-3 bg-danger text-white\">{error.message}</div>\n      )}\n    </div>\n  );\n};\n\nexport default SkillsList;\n","C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\index.js",[],["182","183"],"C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\App.js",[],"C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\reportWebVitals.js",[],["184","185"],"C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\utils\\queries.js",[],"C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\utils\\auth.js",[],"C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\utils\\mutations.js",[],"C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\components\\Navbar.js",[],"C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\utils\\localStorage.js",[],"C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\utils\\API.js",[],"C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\pages\\LandingPage.js",[],"C:\\Users\\Michael\\Documents\\UWA BOOTCAMP\\HOMEWORK REPOSITORY\\Project-3-NAME-TBA\\client\\src\\pages\\Bills.js",["186","187"],"import { Layout } from \"antd\";\r\nimport { Navig